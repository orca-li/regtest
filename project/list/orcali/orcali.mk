VENDOR := orcali

TARGET_CPU ?= stm32f401ccu6

ADDONS ?= 1
ifeq ($(ADDONS),1)
EXIT_BANNER ?= 1
PROGRESS_DUMP ?= 1
REGTEST_BANNER ?= 1
GET_INFO ?= 1
else
EXIT_BANNER ?= 0
PROGRESS_DUMP ?= 0
REGTEST_BANNER ?= 0
GET_INFO ?= 0
endif

ifeq ($(TARGET_CPU),stm32f401ccu6)
GETINFO_CPUNAME := stm32f401ccu6
GETINFO_CPUFREQ := 16MHz
GETINFO_CPUCORE := Cortex-M4
GETINFO_CPUVENDOR := STMicroelectronics
GETINFO_RAMNAME := stm32f401ccu6
GETINFO_RAMVENDOR := STMicroelectronics
GETINFO_RAMSIZE := 64KiB
endif

EMULATOR ?= 1
DECELERATION ?= 1